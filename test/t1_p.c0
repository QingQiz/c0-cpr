const int MAXN = 10, MAXM = 20;

int a[10], b[20];

void init() {
    int i;
    for (i = 0; i < MAXN; i = i + 1)
        a [ i ] = i;

    for (i = 0; i < MAXM; i = i + 1) {
        do b[i] = a[i / 2]; while (0);
    }

    return;
}

int calc(int zero1, int zero2) {
    int i;
    for (i = 0; i < MAXN; ) {
        zero1 = zero1 + a[i] * b[i] / (i + 1 - 2 - 3);
        i = i + 1;
    }
    for (; i < MAXM; i = i + 1) {
        zero2 = zero2 + zero1 + i - MAXN;
    }

    return (zero1 * zero2 - zero2 - zero1 - zero1 / zero2);
}

void main() {
    int z1, z2;
    init();
    scanf(z1, z2);
    printf(calc(z1, z2));
}


