int main () {
    int a, b;

    scanf(a, b);
    printf("a = %d\n", a);
    printf("b = %d\n", b);

    printf("a + b = %d\n", a + b);
    printf("a - b = %d\n", a - b);
    printf("a * b = %d\n", a * b);
    if (b != 0)
    printf("a / b = %d\n", a / b);
    printf("a || b = %d\n", a || b);
    printf("a && b = %d\n", a && b);
    printf("a > b = %d\n", a > b);
    printf("a < b = %d\n", a < b);
    printf("a >= b = %d\n", a >= b);
    printf("a <= b = %d\n", a <= b);
    printf("a == b = %d\n", a == b);
    printf("a != b = %d\n", a != b);
    printf("!a = %d\n", !a);
    printf("!b = %d\n", !b);
}
