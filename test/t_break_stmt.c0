int main() {
    int x;
    int n;

    x = 2;
    for (;;) {
        scanf(n);
        if (n > x) printf(">\n");
        else if (n < x) printf("<\n");
        else {printf("correct!\n");break;}
    }

    x = 128;
    do {
        scanf(n);
        if (n > x) printf(">\n");
        else if (n < x) printf("<\n");
        else {printf("correct!\n");break;}
    } while (1);
}
